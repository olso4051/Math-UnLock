    <!-- The screen names that will appear in reports -->
    <screenName name="com.olyware.mathlock.PacksFrgment">Store Screen</screenName>
    <screenName name="com.olyware.mathlock.SettingsActivity">Settings Screen</screenName>
    <screenName name="com.olyware.mathlock.AdvanceSettingActivity">Advance Settings Screen</screenName>
    <screenName name="com.olyware.mathlock.ShowProgressActivity">Progress Screen</screenName>
    <screenName name="com.olyware.mathlock.MainActivity">Home Screen</screenName>

</resources>