<resources xmlns:tools="http://schemas.android.com/tools">

    <span style="line-height: 1.5; font-style: inherit; font-weight: inherit;">tools:ignore="TypographyDashes"></span>
    <!-- The apps Analytics Tracking Id -->
    <string name="ga_trackingId">UA-48722021-1</string>

    <!-- Percentage of events to include in reports -->
